{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "overrides": [
        {
            "files": "*.yml",
            "options": {
                "tadWidth": 1,
                "printWidth": 40,
                "singleQuote": true
            }
        },
        {
            "files": "*.json",
            "options": {
                "tadWidth": 4,
                "parser": "json",
                "printWidth": 40,
                "singleQuote": false
            }
        }
    ]
}
